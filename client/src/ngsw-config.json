{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-performance",
      "urls": [
          "/assets/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": "100",
        "maxAge": "3d"
      }
    },
    {
      "name": "api_freshness",
      "urls": [
          "/api/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": "100",
        "maxAge": "3d",
        "timeOut": "10s"
      }
    }
  ]
}
